{
    "name": "repat/laravel-helper",
    "description": "Some helper function for developing applications in laravel",
    "keywords": ["laravel", "helper", "files", "array", "database", "strings"],
    "homepage": "http://repat.de",
    "license": "MIT",
        "version" : "0.1.9",
    "authors": [
        {"name": "repat", "email": "repat@repat.de"}
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "5.*",
        "illuminate/database": "5.*",
        "ext-curl": "*"
    },
    "autoload": {
        "files": [
            "src/defines.php",
            "src/array_helpers.php",
            "src/database_helpers.php",
            "src/date_helpers.php",
            "src/html_helpers.php",
            "src/misc_helpers.php",
            "src/networking_helpers.php",
            "src/object_helpers.php",
            "src/optional_packages_helpers.php",
            "src/string_helpers.php"
        ]
    },
    "minimum-stability" : "dev",
    "suggest": {
        "dimsav/laravel-translatable": "Required to use translated_attributes()",
        "league/commonmark" : "Required to use markdown2html()"
    }
}
